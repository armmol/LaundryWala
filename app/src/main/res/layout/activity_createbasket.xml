<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:id="@+id/create_basket">

        <ImageView
            android:id="@+id/imgbtn_carpets"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/carpets_rugs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.194"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:srcCompat="@drawable/carpet" />

        <ImageView
            android:id="@+id/imgbtn_kgs"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/carpets_rugs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:srcCompat="@drawable/kgs" />

        <ImageView
            android:id="@+id/imgbtn_bedsheets"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:contentDescription="@string/bedsheets_duvets"
            app:layout_constraintBottom_toBottomOf="@+id/imgbtn_carpets"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.814"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imgbtn_carpets"
            app:layout_constraintVertical_bias="0.635"
            app:srcCompat="@drawable/bedsheet" />

        <ImageView
            android:id="@+id/imgbtn_suits"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/suits_blazers_coats"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_shirts_add"
            app:layout_constraintHorizontal_bias="0.194"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_shirts_add"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:srcCompat="@drawable/suit" />

        <ImageView
            android:id="@+id/imgbtn_jackets"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:contentDescription="@string/jackets_woolens"
            app:layout_constraintBottom_toBottomOf="@+id/imgbtn_suits"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.814"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imgbtn_suits"
            app:layout_constraintVertical_bias="0.359"
            app:srcCompat="@drawable/jacket" />

        <ImageView
            android:id="@+id/imgbtn_pants"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:contentDescription="@string/pants_any"
            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.814"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imgbtn_shirts_add"
            app:layout_constraintVertical_bias="0.081"
            app:srcCompat="@drawable/pant" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/suits_blazers_coats"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_suits"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_suits"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_suits" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/carpets_rugs"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_carpets"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_carpets"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_carpets" />

        <TextView
            android:id="@+id/textView_kgcounter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView17"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_bedsheets"
            app:layout_constraintHorizontal_bias="0.481"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_bedsheets"
            app:layout_constraintTop_toTopOf="@+id/imgbtn_kgs" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/add_clothes_in_kgs_0_5_kg"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_kgs"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_kgs"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_kgs" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/bedsheets_duvets"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_bedsheets"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_bedsheets"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_carpets" />

        <ImageView
            android:id="@+id/imgbtn_shirts_add"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:contentDescription="@string/shirts"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.19"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.024"
            app:srcCompat="@drawable/shirt" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/shirts_any"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_shirts_add"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_shirts_add"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_shirts_add" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/pants_any"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_pants"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_pants"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_shirts_add" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/jackets_woolens"
            android:textColor="@color/purple_500"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/imgbtn_pants"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="@+id/imgbtn_pants"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_suits"
            app:layout_constraintVertical_bias="1.0" />

        <androidx.cardview.widget.CardView
            android:id="@+id/card_laundrybasket"
            android:layout_width="178dp"
            android:layout_height="109dp"
            app:cardBackgroundColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.07"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.89">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/laundryhouseadapterbackground">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/laundry_basket"
                    android:textColor="@color/purple_500"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/imageView6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.484" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="97dp"
                    android:layout_height="65dp"
                    android:contentDescription="@string/laundry_basket"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.737"
                    app:srcCompat="@drawable/laundrybasket" />

                <TextView
                    android:id="@+id/txt_laundrybasketcounter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="3dp"
                    android:text="@string/_0"
                    android:textColor="@color/purple_500"
                    android:textSize="50sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView6"
                    app:layout_constraintTop_toBottomOf="@+id/textView9" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="29dp"
            android:text="@string/order_summary"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toTopOf="@+id/card_laundrybasket"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.175"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgbtn_kgs"
            app:layout_constraintVertical_bias="0.265" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15 min Drying"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="@+id/checkBox_iNeedDrying"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/checkBox_iNeedDrying"
            app:layout_constraintTop_toBottomOf="@+id/checkBox_iNeedACourier" />

        <Button
            android:id="@+id/btn_confrimandpay_createbasket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/confirm_and_pay"
            app:layout_constraintBottom_toBottomOf="@+id/card_laundrybasket"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.458"
            app:layout_constraintStart_toEndOf="@+id/card_laundrybasket"
            app:layout_constraintTop_toTopOf="@+id/card_laundrybasket"
            app:layout_constraintVertical_bias="0.989" />

        <TextView
            android:id="@+id/txt_laundrybasketcost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_0"
            android:textColor="@color/purple_500"
            android:textSize="50sp"
            app:layout_constraintBottom_toTopOf="@+id/btn_confrimandpay_createbasket"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.46"
            app:layout_constraintStart_toEndOf="@+id/card_laundrybasket"
            app:layout_constraintTop_toTopOf="@+id/card_laundrybasket"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/deliveryprice_createbasket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toBottomOf="@+id/checkBox_iNeedACourier"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.472"
            app:layout_constraintStart_toEndOf="@+id/checkBox_iNeedACourier"
            app:layout_constraintTop_toTopOf="@+id/checkBox_iNeedACourier" />

        <TextView
            android:id="@+id/dryingprice_createbasket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toBottomOf="@+id/checkBox_iNeedDrying"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.472"
            app:layout_constraintStart_toEndOf="@+id/checkBox_iNeedDrying"
            app:layout_constraintTop_toTopOf="@+id/checkBox_iNeedDrying"
            app:layout_constraintVertical_bias="0.48" />

        <CheckBox
            android:id="@+id/checkBox_iNeedACourier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="@string/i_need_a_courier"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="@+id/textView13"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <CheckBox
            android:id="@+id/checkBox_iNeedDrying"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/i_need_drying"
            android:textColor="@color/purple_500"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="@+id/textView13"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView20" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>